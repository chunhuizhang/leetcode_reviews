   "source": [
    "from functools import cache\n",
    "class Solution:\n",
    "    @cache\n",
    "    def fib(self, n: int) -> int:\n",
    "        if n <= 1: return n\n",
    "        return self.fib(n-1) + self.fib(n-2)"
   ]
  }
